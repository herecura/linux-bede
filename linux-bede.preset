# mkinitcpio preset file for linux-bede

########################################
# DO NOT EDIT THIS LINE:
source /etc/mkinitcpio.d/linux-bede.kver
########################################
ALL_config="/etc/mkinitcpio.conf"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-bede.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-bede-fallback.img"
fallback_options="-S autodetect"
